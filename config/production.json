{
  "host": "color-schemer-api-app.feathersjs.com",
  "port": 80,
  "postgres": {
    "database": "POSTGRES_DB",
    "username": "POSTGRES_USER",
    "password": "POSTGRES_PASSWORD"
  },
  "public": "../public/",
  "auth": {
    "idField": "id",
    "token": {
      "secret": "FEATHERS_AUTH_SECRET"
    },
    "local": {},
